# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

inhibit_all_warnings!

target 'Qorum' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Swift pods
    pod 'ReachabilitySwift'
    pod 'SwiftyJSON'
    pod 'DeviceKit'
    
    pod 'Moya'
    
    # UI
    pod 'PINRemoteImage'
    pod 'SDWebImage'
    pod 'SnapKit'
    pod 'IGListKit'
    pod 'SMPageControl'
    pod 'VisualEffectView'
    
    # Facebook
    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'FacebookShare'
    
    # Twitter
    pod 'TwitterKit'
    
    # Fabric
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'ZendeskSDK'
    pod 'ZendeskSDK/Providers'
    
    # GOOGLE
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    
    # PAYMENTS
    pod 'Stripe'
    pod 'CardIO'
    
    # UBER
    pod 'UberCore', :git => 'https://github.com/vriznychok/rides-ios-sdk.git', :branch => 'master'
    pod 'UberRides', :git => 'https://github.com/vriznychok/rides-ios-sdk.git', :branch => 'master'
    
    # MISC
    pod 'PhoneNumberKit'
    pod 'Firebase/Core'
    pod 'Firebase/Messaging'
    pod 'Mixpanel-swift'
    pod 'Branch'
    
    # Logging
    pod 'SwiftyBeaver'
    
end

target 'Qorum Config' do
    use_frameworks!
    pod 'Fabric'
    pod 'Crashlytics'
end
